package logic;

/**
 *
 * @author Reyhan
 */
public class MisterX extends Player{
    int numOfTurns;
    public MisterX(int id, playerType type, boolean isAI, int numOfTaxis, 
            int numOfBuses, int numOfBoats, int numOfTrains) {
        super(id, type, isAI, numOfTaxis, numOfBuses, numOfBoats, numOfTrains);
          this.numOfTurns =0;
    }
    @Override
    protected int getnumOfTurns(){
        return numOfTurns;
    }
    
    @Override
    protected void incNumOfTurns(){
        numOfTurns++;
    }
    
}
